{
    "name": "tahamohamed/smsgateway",
    "description": "Send SMS using multiple SMS gateways",
    "license": "MIT",
    "authors": [
        {
            "name": "Taha Mohamed",
            "email": "taha.mohamed2016@gmail.com",
            "homepage": "https://github.com/TahaMohamed"
        }
    ],
    "homepage": "https://github.com/TahaMohamed/smsgateway",
    "keywords": ["Laravel", "SMSGateway", "sms", "hisms"],
    "require": {
        "illuminate/support": "^5.0|^6.0|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0",
        "orchestra/testbench": ">=3.8"
    },
    "autoload": {
        "psr-4": {
            "TahaMohamed\\SMSGateway\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TahaMohamed\\SMSGateway\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TahaMohamed\\SMSGateway\\SMSGatewayServiceProvider"
            ],
            "aliases": {
                "SMSGateway": "TahaMohamed\\SMSGateway\\Facades\\SMSGateway",
                "Hisms": "TahaMohamed\\SMSGateway\\Gateways\\Hisms",
                "NetPowers": "TahaMohamed\\SMSGateway\\Gateways\\NetPowers",
                "smsGateway": "TahaMohamed\\SMSGateway\\Gateways\\smsGateway"
            }
        }
    }
}
